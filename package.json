{
	"name": "kingdom-study",
	"version": "1.0.0",
	"description": "GUI 2 Group Project",
	"main": "server.js",
	"engines": {
		"node": "16.15.0"
	},
	"scripts": {
		"build": "(cd frontend && npm run build) && cd ..",
		"start": "node backend/server.js",
		"server": "nodemon backend/server.js",
		"client": "npm start --prefix frontend",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"heroku-build-deploy": "npm run build && (git add . && git commit -m \"attempting deployment to heroku\") && git push heroku master",
		"heroku-postpuild": "NODE_ENV=production npm install --prefix frontend && npm run build --prefix frontend"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AnsonCheang50/Kingdom-Study.git"
	},
	"author": "Group 4",
